{
  "apps": [
    {
      "name": "nodejs-simple-demo",
      "script": "./index.js",
      "watch": true,
      "ignore_watch" : [
        "node_modules",
        "logs",
        "mock"
      ],
      "instances": 4,
      "node_args": "--harmony",
      "exec_mode": "cluster",
      "error_file" : "./logs/app-err.log",
      "out_file"   : "./logs/app-out.log"
    }
  ]
}
