{
  "apps": [
    {
      "name": "nodejs-simple-demo",
      "script": "./app/routes/index.js",
      "watch": [
        "app"
      ],
      "ignore_watch": [
        "node_modules",
        "logs",
        "mock"
      ],
      "instances": 4,
      "node_args": "--harmony",
      "exec_mode": "cluster",
      "error_file": "./logs/nodejs-simple-demo-err.log",
      "out_file": "./logs/nodejs-simple-demo-out.log"
    }
  ]
}
